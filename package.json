{
  "name": "orchardblog",
  "description": "Lessons blog client based on sapper",
  "version": "0.1.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export && cp __sapper__/export/index.html __sapper__/export/200.html",
